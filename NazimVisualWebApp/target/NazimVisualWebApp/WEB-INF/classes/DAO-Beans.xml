<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans
	    http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">

	<bean id="userDAO" class="cmpe.boun.NazimVisualize.DAO.UserDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>
	
	<bean id="BookDao" class="cmpe.boun.NazimVisualize.DAO.BookDao">
		<property name="dataSource" ref="dataSource" />
	</bean>
	
	<bean id="WordDao" class="cmpe.boun.NazimVisualize.DAO.WordDao">
		<property name="dataSource" ref="dataSource" />
	</bean>
	
	<bean id="WorkDao" class="cmpe.boun.NazimVisualize.DAO.WorkDao">
		<property name="dataSource" ref="dataSource" />
	</bean>
	
	<bean id="WorkLineDao" class="cmpe.boun.NazimVisualize.DAO.WorkLineDao">
		<property name="dataSource" ref="dataSource" />
	</bean>

</beans>